{
    "compilerOptions": {
        "target": "es6",
        "lib": ["es6", "dom"],
        "module": "commonjs",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "types": ["cypress", "./cypress/support/commands", "node"],
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        
    },
    "include": ["**/*.ts", "support/commands.ts"]
}
